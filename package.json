{
  "name": "mf-cockpit-data-puller",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ts-node --transpile-only --files ./server/index.ts",
    "build": "ng build --prod",
    "build-staging": "ng build --prod",
    "start:local": "run-s serve:single-spa",
    "start:dev": "run-s build:single-spa & ts-node --files ./server/index.ts",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:single-spa": "ng build mf-cockpit-data-puller --configuration production",
    "serve:single-spa": "ng s --project mf-cockpit-data-puller --disable-host-check --port 4205 --deploy-url http://localhost:4205/ --live-reload false"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.2",
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.2",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.2",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "compression": "^1.7.4",
    "express": "^4.17.3",
    "hsts": "^2.2.0",
    "moment": "^2.29.2",
    "path": "^0.12.7",
    "rxjs": "~7.5.0",
    "single-spa": ">=4.0.0",
    "single-spa-angular": "^6.1.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.1.0",
    "@angular-devkit/build-angular": "~13.3.1",
    "@angular/cli": "~13.3.1",
    "@angular/compiler-cli": "~13.3.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.1",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.2"
  }
